{
  "name": "contador-solo-ai",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:webpack": "next dev",
    "build": "next build --turbopack",
    "build:webpack": "next build",
    "build:strict": "npm run lint:prod && npm run type-check && npm run build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "lint:dev": "eslint --config .eslintrc.development.js .",
    "lint:prod": "eslint --config .eslintrc.production.js .",
    "lint:unused": "npx unimported",
    "clean:imports": "npx organize-imports-cli tsconfig.json src/**/*.{ts,tsx}",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "analyze": "set ANALYZE=true && npm run build:webpack",
    "analyze:server": "set BUNDLE_ANALYZE=server && npm run build:webpack",
    "analyze:browser": "set BUNDLE_ANALYZE=browser && npm run build:webpack",
    "supabase:types": "supabase gen types typescript --project-id selnwgpyjctpjzdrfrey > src/types/database.types.ts",
    "workers:start": "tsx scripts/start-workers.ts",
    "workers:dev": "tsx watch scripts/start-workers.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "test:ci": "jest --coverage --watchAll=false --passWithNoTests",
    "test:e2e": "jest --config jest.config.e2e.js",
    "test:e2e:watch": "jest --config jest.config.e2e.js --watch",
    "test:e2e:coverage": "jest --config jest.config.e2e.js --coverage",
    "test:e2e:full": "tsx scripts/run-e2e-tests.ts",
    "test:e2e:report": "tsx scripts/run-e2e-tests.ts && open test-reports/e2e-report.html",
    "test:ocr": "jest --testPathPatterns=ocr",
    "test:ocr:unit": "jest --testPathPatterns=ocr.*unit",
    "test:ocr:integration": "jest --testPathPatterns=ocr.*integration",
    "test:ocr:e2e": "jest --testPathPatterns=ocr.*e2e",
    "test:ocr:precision": "jest --testPathPatterns=ocr.*precision",
    "test:ocr:all": "jest --testPathPatterns=ocr --coverage --verbose",
    "test:ocr:watch": "jest --testPathPatterns=ocr --watch",
    "test:ocr:ci": "jest --testPathPatterns=ocr --ci --coverage --watchAll=false --maxWorkers=2"
  },
